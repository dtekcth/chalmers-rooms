{
  "name": "dudsman",
  "version": "0.0.0",
  "private": true,
  "main": "src/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "clean": "rm -rf dist && mkdir dist",
    "start:watch": "nodemon src/app.js --exec 'babel-node --presets @babel/preset-env,@babel/preset-react'",
    "build:server": "babel src --presets @babel/preset-env,@babel/preset-react --out-dir dist",
    "build:css": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build": "npm run clean && npm run build:server && npm run build:css",
    "watch:dev": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "files": [
    "dist/",
    "public/"
  ],
  "nodemonConfig": {
    "ignore": [
      "src/client/",
      "public/",
      "mix-manifest.json"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "any-promise": "^1.3.0",
    "autobind-decorator": "^2.1.0",
    "bluebird": "^3.5.1",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.6",
    "cookie-parser": "~1.4.3",
    "date-fns": "^1.29.0",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "get-form-data": "^2.0.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.13",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "query-string": "^6.2.0",
    "rc-time-picker": "^3.4.0",
    "react": "^16.4.1",
    "react-autosuggest": "^9.4.2",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-mounter": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.0.1",
    "babel-core": "7.0.0-bridge.0",
    "browser-sync": "^2.24.6",
    "browser-sync-webpack-plugin": "2.0.1",
    "eslint": "^5.2.0",
    "eslint-plugin-babel": "^5.1.0",
    "glob-all": "^3.1.0",
    "laravel-mix": "^3.0.0",
    "node-sass": "^4.13.1",
    "postcss-color-function": "^4.0.1",
    "postcss-momentum-scrolling": "^1.4.8",
    "tailwindcss": "^0.6.4"
  }
}
